<<div class="container">
  <h1>Students Details</h1>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">First Name</th>
        <th scope="col">Last Name</th>
        <th scope="col">Age</th>
        <th scope="col">Action</th>
      </tr>
    </thead>
    <tbody>
      {{#each data}}
      <tr>
        <th scope="row">{{this.id}}</th>
        <td>{{this.firstName}}</a></td>
        <td>{{this.lastName}}</td>
        <td>{{this.age}}</td>
        <td><a href="/edit-student/{{this.id}}">Edit</a></td>
      </tr>
      {{/each}}
    </tbody>
  </table>
  <a class="btn btn-primary btn-lg" href="/students/add-student">Add student</a>
</div>
